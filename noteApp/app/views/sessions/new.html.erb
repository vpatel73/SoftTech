<h1>Sign In</h1>

    <%= form_for(:session, url: login_path) do |f| %>
        
        <%= f.label :email %>
        <%= f.email_field :email %>
        
        <%= f.label :password %>
        <%= f.password_field :password %>
        
        <%= f.check_box :remember_me %>
            <span> Remember me</span>
        
       <%= f.submit "Sign in" %>
        
    <% end %>